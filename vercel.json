{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "includeFiles": [
    "node_modules/.prisma/**",
    "node_modules/@prisma/client/**",
    "prisma/schema.prisma"
  ]
}
